  </ItemGroup>

  <ItemGroup>
    <Content Include="Resources\Eraser.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="Resources\Star.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
