    <None Remove="Resources\ColorPicker.png" />
    <None Remove="Resources\Ellipse.png" />
    <None Remove="Resources\Eraser.png" />
    <None Remove="Resources\Line.png" />
    <None Remove="Resources\LineWidth.png" />
    <None Remove="Resources\Pen.png" />
    <None Remove="Resources\Star.png" />
    <None Remove="Resources\ZoomIn.png" />
    <None Remove="Resources\ZoomOut.png" />
  </ItemGroup>

